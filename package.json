        "zod": "^4.1.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "@types/node": "^20.19.11",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@types/sharp": "^0.31.1",
        "eslint": "^9",
        "eslint-config-next": "15.5.0",
        "tailwindcss": "^4",
        "typescript": "^5"
    }
}